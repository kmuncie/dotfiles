{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#bf616a",
          "foreground": "#3b4252",
          "leading_diamond": "\ue0b6",
          "properties": {
            "windows": "\ue62a"
          },
          "style": "diamond",
          "template": "{{ if .WSL }}WSL at {{ end }}{{.Icon}}",
          "type": "os"
        },
        {
          "background": "#bf616a",
          "foreground": "#3b4252",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "style": "full"
          },
          "style": "powerline",
          "template": "{{ .Path }} ",
          "type": "path"
        },
        {
          "background": "#d08770",
          "foreground": "#3b4252",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "type": "node"
        },
        {
          "background": "#d08770",
          "foreground": "#3b4252",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "fetch_version": false
          },
          "style": "powerline",
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
          "type": "python"
        },
        {
          "background": "#8fbcbb",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#ebcb8b{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#d08770{{ end }}",
            "{{ if gt .Ahead 0 }}#a3be8c{{ end }}",
            "{{ if gt .Behind 0 }}#d08770{{ end }}"
          ],
          "foreground": "#3b4252",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if .UpstreamGone }} \uf00d{{ else }}{{ if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ end }}{{ if .Upstream }} \uf126 {{ .Upstream }}{{ end }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "#b48ead",
          "foreground": "#3b4252",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "properties": {
            "always_enabled": true
          },
          "style": "powerline",
          "template": " {{ .FormattedMs }} ",
          "type": "executiontime"
        },
        {
          "background": "#4c566a",
          "foreground": "#8fbcbb",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "style": "powerline",
          "template": " \u26a1",
          "type": "root"
        },
        {
          "background": "#86BBD8",
          "foreground": "#3b4252",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "style": "powerline",
          "template": " {{ .UserName }} ",
          "type": "session"
        },
        {
          "background": "#33658A",
          "foreground": "#3b4252",
          "invert_powerline": true,
          "style": "diamond",
          "template": " {{ .Name }} ",
          "trailing_diamond": "\ue0b4",
          "type": "shell"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "style": "plain",
          "template": " \uf0e7 ",
          "type": "root"
        },
        {
          "foreground": "#3b4252",
          "style": "plain",
          "template": "<#a3be8c>\u276f</>",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
